{
  "name": "asia",
  "version": "0.12.0",
  "description": "Blazingly fast, magical and minimalist testing framework, for Today and Tomorrow",
  "repository": "olstenlarck/asia",
  "author": "Charlike Mike Reagent",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "bin": "src/cli.js",
  "scripts": {
    "test": "nyc node test/index.js",
    "lint": "xaxa",
    "commit": "gitcommit -sS"
  },
  "eslintConfig": {
    "extends": "xaxa",
    "rules": {
      "strict": "off"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "cache": true,
    "exclude": [
      "src/{cli,index}.js",
      "src/reporters",
      "test"
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0-beta.52",
    "ansi-colors": "^2.0.2",
    "arrayify": "^1.0.0",
    "clean-stacktrace": "^1.1.0",
    "color-support": "^1.1.3",
    "execa": "^0.10.0",
    "export-files": "^2.1.1",
    "fast-glob": "^2.2.2",
    "mri": "^1.1.1",
    "p-map": "^1.2.0",
    "p-map-series": "^1.0.0",
    "p-reflect": "^1.0.0",
    "serialize-error": "^2.1.0"
  },
  "devDependencies": {
    "gitcommit": "^1.0.7",
    "mukla": "^0.4.9",
    "nyc": "^12.0.2",
    "xaxa": "^1.3.6"
  }
}
