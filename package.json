{
  "name": "asia",
  "version": "0.19.7",
  "description": "Blazingly fast, magical and minimalist testing framework, for Today and Tomorrow",
  "repository": "olstenlarck/asia",
  "author": "Charlike Mike Reagent",
  "license": "Apache-2.0",
  "licenseStart": 2017,
  "main": "src/index.js",
  "bin": "src/cli.js",
  "files": [
    "LICENSE",
    "src"
  ],
  "engines": {
    "node": "^8.9.0 || ^10.6.0"
  },
  "scripts": {
    "lint": "xaxa",
    "test": "nyc node src/cli.js",
    "precommit": "yarn lint && yarn test && git status --porcelain && git add .",
    "commit": "gitcommit -sS",
    "cm": "git status --porcelain && git add . && gitcommit -sS"
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0-beta.52",
    "ansi-colors": "^3.0.0",
    "arrayify": "^1.0.0",
    "clean-stacktrace": "^1.1.0",
    "fast-glob": "^2.2.2",
    "globrex": "^0.1.1",
    "is-ci": "^2.0.0",
    "is-observable": "^2.0.0",
    "mkdirp": "^0.5.1",
    "mri": "^1.1.1",
    "observable-to-promise": "^1.0.0",
    "p-map": "^1.2.0",
    "p-map-series": "^1.0.0",
    "p-reflect": "^1.0.0",
    "pretty-ms": "^3.2.0",
    "rimraf": "^2.6.2",
    "serialize-error": "^2.1.0",
    "supports-color": "^5.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "asia": "^0.19.0",
    "gitcommit": "^1.0.7",
    "nyc": "^14.0.0",
    "xaxa": "^1.3.6",
    "zen-observable": "^0.8.8"
  },
  "eslintConfig": {
    "extends": "xaxa"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "src/{cli,index}.js",
      "src/reporters",
      "test"
    ]
  },
  "verb": {
    "reflinks": [
      "ghrelease-img",
      "ghrelease-url",
      "new-release"
    ]
  }
}
